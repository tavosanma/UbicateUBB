<resources>

    <!-- Base application theme. -->
    <style name="sinbar" parent="Theme.AppCompat.Light.NoActionBar">

        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="conbar" parent="Theme.AppCompat.Light.DarkActionBar">

        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/negro</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="sinbar.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="sinbar.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


</resources>
